begin
  Result := '';
  for i := 0 to KDT96DI64_Axis - 1 do
    begin
      if i > 0 then
          Result := Result + ',';
      Result := Result + umlIntToStr(v[i]);
    end;
end;

class function TKDT96DI64.Distance(const v1, v2: TKDT96DI64_Vec): Double;

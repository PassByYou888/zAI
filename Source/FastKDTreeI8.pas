class procedure TKDT64DI8.Test;
var
  TKDT64DI8_Test: TKDT64DI8;
  t: TTimeTick;
  i, j: NativeInt;
  TestResultBuff: TKDT64DI8_DynamicVecBuffer;
  TestResultIndex: TKMIntegerArray;
  KMeanOutIndex: TKMIntegerArray;
  errored: Boolean;
  m64: TMemoryStream64;
  p: PKDT64DI8_Node;
  n: TPascalString;
begin
  errored := False;
  n := PFormat('test %s...', [ClassName]);
  t := GetTimeTick;

  n.Append('...build');
  TKDT64DI8_Test := TKDT64DI8.Create;

  n.Append('...');
  SetLength(TKDT64DI8_Test.TestBuff, 1000);
  for i := 0 to length(TKDT64DI8_Test.TestBuff) - 1 do
    for j := 0 to KDT64DI8_Axis - 1 do
        TKDT64DI8_Test.TestBuff[i][j] := i * KDT64DI8_Axis + j;

{$IFDEF FPC}
  TKDT64DI8_Test.BuildKDTreeM(length(TKDT64DI8_Test.TestBuff), nil, @TKDT64DI8_Test.Test_BuildM);
{$ELSE FPC}
  TKDT64DI8_Test.BuildKDTreeM(length(TKDT64DI8_Test.TestBuff), nil, TKDT64DI8_Test.Test_BuildM);
{$ENDIF FPC}
  { save/load test }
  n.Append('...save/load');
  m64 := TMemoryStream64.CustomCreate(1024 * 1024);
  TKDT64DI8_Test.SaveToStream(m64);
  m64.Position := 0;
  TKDT64DI8_Test.LoadFromStream(m64);
  for i := 0 to length(TKDT64DI8_Test.TestBuff) - 1 do
    begin
      p := TKDT64DI8_Test.Search(TKDT64DI8_Test.TestBuff[i]);
      if p^.Vec^.Index <> i then
          errored := True;
      if not p^.Vec^.Token.Same(umlIntToStr(i)) then
          errored := True;
      if errored then
          Break;
    end;
  DisposeObject(m64);

  if not errored then
    begin
      { parallel search test }
      n.Append('...parallel');
      SetLength(TestResultBuff, length(TKDT64DI8_Test.TestBuff));
      SetLength(TestResultIndex, length(TKDT64DI8_Test.TestBuff));
      TKDT64DI8_Test.Search(TKDT64DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

      for i := 0 to length(TestResultIndex) - 1 do
        if Distance(TKDT64DI8_Test.TestBuff[TestResultIndex[i]], TestResultBuff[TestResultIndex[i]]) <> 0 then
            errored := True;
    end;

  if not errored then
    begin
      n.Append('...kMean');
      TKDT64DI8_Test.Clear;
      { kMean test }
      TKDT64DI8_Test.BuildKDTreeWithCluster(TKDT64DI8_Test.TestBuff, 10, 1, KMeanOutIndex);
      { parallel search test }
      TKDT64DI8_Test.Search(TKDT64DI8_Test.TestBuff, TestResultBuff, TestResultIndex);

      for i := 0 to length(TestResultIndex) - 1 do
        if TestResultIndex[i] <> KMeanOutIndex[i] then
            errored := True;
    end;

  SetLength(TKDT64DI8_Test.TestBuff, 0);
  SetLength(TestResultBuff, 0);
  SetLength(TestResultIndex, 0);
  SetLength(KMeanOutIndex, 0);
  TKDT64DI8_Test.Clear;

  n.Append('...');
  if errored then
      n.Append('error!')
  else
      n.Append('passed ok %dms', [GetTimeTick - t]);
  DisposeObject(TKDT64DI8_Test);
  DoStatus(n);
  n := '';
end;

function TKDT96DI8.InternalBuildKdTree(const KDSourceBufferPtr: PKDT96DI8_SourceBuffer; const PlanCount, Depth: NativeInt): PKDT96DI8_Node;
  function SortCompare(const p1, p2: PKDT96DI8_Source; const axis: NativeInt): ShortInt;
  begin
    if p1^.buff[axis] = p2^.buff[axis] then
      begin
        if p1^.Index = p2^.Index then
            Result := 0
        else if p1^.Index < p2^.Index then
            Result := -1
        else
            Result := 1;
      end
    else if p1^.buff[axis] < p2^.buff[axis] then
        Result := -1
    else
        Result := 1;
  end;

  procedure InternalSort(const SortBuffer: PKDT96DI8_SourceBuffer; L, R: NativeInt; const axis: NativeInt);
  var
    i, j: NativeInt;
    p, t: PKDT96DI8_Source;
  begin
    repeat
      i := L;
      j := R;
      p := SortBuffer^[(L + R) shr 1];
      repeat
        while SortCompare(SortBuffer^[i], p, axis) < 0 do
            Inc(i);
        while SortCompare(SortBuffer^[j], p, axis) > 0 do
            Dec(j);
        if i <= j then
          begin
            if i <> j then
              begin
                t := SortBuffer^[i];
                SortBuffer^[i] := SortBuffer^[j];
                SortBuffer^[j] := t;
              end;
            Inc(i);
            Dec(j);
          end;
      until i > j;
      if L < j then
          InternalSort(SortBuffer, L, j, axis);
      L := i;
    until i >= R;
  end;

var
  M: NativeInt;
  axis: NativeInt;
  kdBuffPtr: PKDT96DI8_SourceBuffer;
begin
  Result := nil;
  if PlanCount = 0 then
      Exit;

  if PlanCount = 1 then
    begin
      new(Result);
      Result^.Parent := nil;
      Result^.Right := nil;
      Result^.Left := nil;
      Result^.Vec := KDSourceBufferPtr^[0];

      KDNodes[NodeCounter] := Result;
      Inc(NodeCounter);
    end
  else
    begin
      axis := Depth mod KDT96DI8_Axis;
      M := PlanCount div 2;

      kdBuffPtr := GetMemory(PlanCount * SizeOf(Pointer));
      CopyPtr(@KDSourceBufferPtr^[0], @kdBuffPtr^[0], PlanCount * SizeOf(Pointer));

      if PlanCount > 1 then
          InternalSort(@kdBuffPtr^[0], 0, PlanCount - 1, axis);

      new(Result);
      Result^.Parent := nil;
      Result^.Vec := kdBuffPtr^[M];

      KDNodes[NodeCounter] := Result;
      Inc(NodeCounter);

      Result^.Left := InternalBuildKdTree(@kdBuffPtr^[0], M, Depth + 1);
      if Result^.Left <> nil then
          Result^.Left^.Parent := Result;

      Result^.Right := InternalBuildKdTree(@kdBuffPtr^[M + 1], PlanCount - (M + 1), Depth + 1);
      if Result^.Right <> nil then
          Result^.Right^.Parent := Result;

      FreeMemory(kdBuffPtr);
    end;
end;


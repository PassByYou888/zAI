var
  i: NativeInt;
begin
  Result := '';
  for i := 0 to KDT6DI8_Axis - 1 do
    begin
      if i > 0 then
          Result := Result + ',';
      Result := Result + umlIntToStr(v[i]);
    end;
end;

class function TKDT6DI8.Distance(const v1, v2: TKDT6DI8_Vec): Double;
var
  i: NativeInt;
begin
  Result := 0;
  for i := 0 to KDT6DI8_Axis - 1 do
      Result := Result + (v2[i] - v1[i]) * (v2[i] - v1[i]);
end;

